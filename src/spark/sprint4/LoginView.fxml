<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="spark.sprint4.LoginController">
   <children>
      <AnchorPane layoutX="15.0" layoutY="82.0" prefHeight="86.0" prefWidth="371.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Username:" />
            <Label layoutX="14.0" layoutY="53.0" text="Password:" />
            <TextField fx:id="usernameField" layoutX="78.0" layoutY="10.0" prefHeight="25.0" prefWidth="283.0" />
            <PasswordField fx:id="passwordField" layoutX="78.0" layoutY="49.0" onAction="#handleSubmitButtonAction" prefHeight="25.0" prefWidth="283.0" />
         </children>
      </AnchorPane>
      <ButtonBar layoutX="185.0" layoutY="196.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="submitButton" mnemonicParsing="false" onAction="#handleSubmitButtonAction" text="Submit" />
        </buttons>
      </ButtonBar>
      <Text fx:id="actiontarget" layoutX="15.0" layoutY="221.0" strokeType="OUTSIDE" strokeWidth="0.0" />
   </children>
</AnchorPane>
